#include <stdlib.h>
#include <stdio.h>
#include <math.h>
int main()
{


 double x,y;


 printf("Enter x:");
 scanf("%lf", &x);

   if( -3 <= x && x < -1)
      {
          y=exp(x-1);
      }
   else if ( 1 <= x && x < 3 )
      {
          y=exp(x+1);
      }


    else if ( 10 <= x && x < 14 )
      {
          y = -log (x) + x/cos(1*x)+1;
      }

       else
      {
        y = 3;
      }

    system("cls");
        printf("x = %lf", x);
         printf("\ny = %lf", y);
 return 0;
}
