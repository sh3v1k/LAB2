#include <stdlib.h>
#include <stdio.h>
#include <math.h>
int main()
{


 double x;
 double y;


 printf("Enter x:");
 scanf("%lf", &x);

   if( 0 <= x && x < 3 )
        x =1/2* cos(x/2)-1/(x * x)*sin(2*x);
   else if ( 7 <= x && x < 10 )
        x =4-(x*x);
   else if ( 10 <= x && x < 30 )
        x =10/x+(x*x);
       else
        x = 2;

 system("cls");
 printf("x = %lf", x);
 printf("\ny = %lf", y);
 return 0;
